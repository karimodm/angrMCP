{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "solve_constraints response",
  "type": "object",
  "required": ["results"],
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["kind", "value"],
        "properties": {
          "kind": {
            "type": "string",
            "enum": ["memory", "register"]
          },
          "value": {"type": "integer"},
          "min": {"type": "integer"},
          "max": {"type": "integer"}
        },
        "additionalProperties": false
      }
    }
  }
}
