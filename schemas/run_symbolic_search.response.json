{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "run_symbolic_search response",
  "type": "object",
  "required": ["run"],
  "properties": {
    "run": {
      "type": "object",
      "required": [
        "simgr_id",
        "active",
        "deadended",
        "found",
        "avoided",
        "errored",
        "errors",
        "alerts",
        "job_id",
        "stashes"
      ],
      "additionalProperties": false,
      "properties": {
        "simgr_id": {"$ref": "common.json#/$defs/uuid"},
        "job_id": {"$ref": "common.json#/$defs/uuid"},
        "active": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/uuid"}
        },
        "deadended": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/uuid"}
        },
        "found": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/uuid"}
        },
        "avoided": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/uuid"}
        },
        "errored": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/uuid"}
        },
        "errors": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/error-record"}
        },
        "alerts": {
          "type": "array",
          "items": {"$ref": "common.json#/$defs/alert"}
        },
        "stashes": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": {"$ref": "common.json#/$defs/uuid"}
          }
        }
      }
    }
  }
}
